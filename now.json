{
  "name": "discordlist-api",
  "version": 2,
  "builds": [{
    "src": "src/server.js",
    "use": "@now/node-server"
  }],
  "env": {
    "DISCORD_CLIENT_ID": "681457645466419210",
    "DISCORD_CLIENT_SECRET": "@discord-client-secret",
    "DISCORD_CLIENT_TOKEN": "@discord-client-token",
    "DISCORD_OAUTH_REDIRECT": "https://discordlist.pt/login",
    "DISCORD_OAUTH_SCOPE": "identify guilds.join",
    "FRONTEND_URL": "https://discordlist.pt",
    "MONGOOSE_URI": "@mongoose-uri"
  },
  "routes": [
    {
      "src": "/*",
      "dest": "src/server.js"
    }
  ]
}